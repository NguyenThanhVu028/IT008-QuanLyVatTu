<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\AppLogo\logo.png" />
    <None Remove="Images\BackGround\DefaultBackGround.jpg" />
    <None Remove="Images\Icons\close.png" />
    <None Remove="Images\Icons\maximize.png" />
    <None Remove="Images\Icons\minimize.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\AppLogo\logo.png" />
    <Resource Include="Images\BackGround\DefaultBackGround.jpg" />
    <Resource Include="Images\Icons\close.png" />
    <Resource Include="Images\Icons\maximize.png" />
    <Resource Include="Images\Icons\minimize.png" />
  </ItemGroup>

</Project>
